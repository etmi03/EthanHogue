body	{font-family: Arial, Centaur, "Century Gothic";
		font-size: 15pt;
		text-align: center;
		}
		
p		{margin-left: 120px;
		margin-right: 120px;
		}
table, th, td		{margin-left: auto;
					margin-right: auto;
					border: 5px solid black;
					}

.boldword	{color: #030a24; 
			font-weight: bolder;}

img		{border-color: transparent;}

li			{display: inline; 
			list-style-type: none;}
			
a			{text-decoration: none;
			color: #030a24;
			}
		
a:hover		{background: #030a24; 
			color: white;
			}